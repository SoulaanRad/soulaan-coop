{
  "name": "@repo/trpc",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/trpc.js",
  "types": "dist/trpc.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:run": "vitest run",
    "test:ci": "vitest run --reporter=verbose"
  },
  "exports": {
    ".": {
      "import": "./dist/trpc.js",
      "require": "./dist/trpc.js",
      "types": "./dist/trpc.d.ts"
    },
    "./context": {
      "import": "./dist/context.js",
      "require": "./dist/context.js",
      "types": "./dist/context.d.ts"
    },
    "./router": {
      "import": "./dist/routers/index.js",
      "require": "./dist/routers/index.js",
      "types": "./dist/routers/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    },
    "./services/wallet-service": {
      "import": "./dist/services/wallet-service.js",
      "require": "./dist/services/wallet-service.js",
      "types": "./dist/services/wallet-service.d.ts"
    },
    "./services/blockchain": {
      "import": "./dist/services/blockchain.js",
      "require": "./dist/services/blockchain.js",
      "types": "./dist/services/blockchain.d.ts"
    },
    "./services/payment/*": {
      "import": "./dist/services/payment/*.js",
      "require": "./dist/services/payment/*.js",
      "types": "./dist/services/payment/*.d.ts"
    },
    "./services/admin-verification": {
      "import": "./dist/services/admin-verification.js",
      "require": "./dist/services/admin-verification.js",
      "types": "./dist/services/admin-verification.d.ts"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@repo/db": "workspace:*",
    "@repo/validators": "workspace:*",
    "@trpc/server": "catalog:",
    "bcryptjs": "^3.0.2",
    "iron-session": "catalog:",
    "next": "catalog:",
    "next-auth": "^4.24.11",
    "stripe": "^20.1.2",
    "nodemailer": "^7.0.12",
    "superjson": "^2.2.2",
    "viem": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/tsconfig": "workspace:*",
    "@trpc/client": "catalog:",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/nodemailer": "^7.0.4",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
