{
  "name": "@repo/db",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs", 
  "types": "dist/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "directory": "dist"
  },
  "scripts": {
    "build": "pnpm run db:generate && tsup",
    "dev": "pnpm run db:generate && tsup --watch",
    "seed": "ts-node --transpile-only prisma/seed.ts",
    "clean": "rm -rf .turbo node_modules",
    "db:generate": "prisma generate",
    "db:migrate-prod": "pnpm with-env prisma migrate deploy",
    "db:migrate": "pnpm with-env  prisma migrate dev",
    "db:studio": "pnpm with-env prisma studio --browser none",
    "db:push": "pnpm with-env prisma db push --skip-generate",
    "with-env": "dotenv -e ./.env --"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@t3-oss/env-core": "^0.9.2",
    "dotenv": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "dotenv-cli": "catalog:",
    "prisma": "^6.13.0",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  }
}
