{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**", "cache/**", "artifacts/**"],
      "inputs": ["test/**/*.ts", "contracts/**/*.sol", "src/**/*.ts", "src/**/*.tsx"]
    },
    "test:coverage": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**", "cache/**", "artifacts/**"],
      "cache": true
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "push": {
      "cache": false,
      "interactive": true
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    },
    "db:generate": {
      "env": ["DATABASE_URL", "DIRECT_URL"],
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db:push": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db:migrate-prod": {
      "env": ["DATABASE_URL", "DIRECT_URL"],

      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db:migrate": {
      "env": ["DATABASE_URL", "DIRECT_URL"],

      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "db:studio": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    }
  },
  "globalEnv": [
    "POSTGRES_URL",
    "AUTH_REDIRECT_PROXY_URL",
    "AUTH_SECRET",
    "SESSION_SECRET",
    "PORT",
    "OPENAI_API_KEY",
    "SEPOLIA_RPC_URL",
    "MAINNET_RPC_URL",
    "DEPLOYER_PRIVATE_KEY",
    "ETHERSCAN_API_KEY",
    "REPORT_GAS",
    "WALLET_ENCRYPTION_KEY",
    "BACKEND_WALLET_PRIVATE_KEY",
    "RPC_URL",
    "SOULAANI_COIN_ADDRESS",
    "UNITY_COIN_ADDRESS",
    "TREASURY_SAFE_ADDRESS",
    "DATABASE_URL",
    "STRIPE_SECRET_KEY",
    "STRIPE_WEBHOOK_SECRET",
    "TWILIO_ACCOUNT_SID",
    "TWILIO_AUTH_TOKEN",
    "TWILIO_PHONE_NUMBER"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event"
  ]
}
